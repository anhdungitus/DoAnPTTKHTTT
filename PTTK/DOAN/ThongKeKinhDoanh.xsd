            <MainSource>
              <DbSource ConnectionRef="WATERConnectionString (Settings)" DbObjectName="WATER.dbo.ThietBi" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT TB.MaTB, TB.Ten, TB.LoaiThietBi, (TT.SLThieu-TB.SLKho) AS SLThieu
FROM ThietBi TB, (SELECT T.MaTB, SUM(SLThieu) AS SLThieu
		FROM (SELECT *	FROM (SELECT T1.MaTB, SUM(SPM.SLuong - SLDaGiao) AS SLThieu
		FROM (SELECT MaHD, MaTB, SUM(SLDaGiao) SLDaGiao
		FROM CT_GiaoHang CTG
		GROUP BY MAHD, MaTB) AS T1, SanPhamMua SPM
		WHERE SPM.MaHD = T1.MaHD AND SPM.MaTB = T1.MaTB AND SLDaGiao &lt; SPM.SLuong
		GROUP BY T1.MaTB) G1 UNION ALL SELECT * FROM (SELECT T1.MaTB, SUM(SPT.SLuong - SLDaGiao) AS SLThieu
								FROM (SELECT MaHD, MaTB, SUM(SLDaGiao) SLDaGiao
										FROM CT_GiaoHang CTG
										GROUP BY MAHD, MaTB) AS T1, SanPhamThue SPT
								WHERE SPT.MaHD = T1.MaHD AND SPT.MaTB = T1.MaTB AND SLDaGiao &lt; SPT.SLuong
								GROUP BY T1.MaTB) G2) AS T
GROUP BY T.MaTB) AS TT
WHERE TB.MaTB = TT.MaTB AND SLKho &lt; TT.SLThieu</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
